{
    "name": "laravel-route-annotations",
    "displayName": "Laravel Route Annotations",
    "description": "Shows route path annotations for Laravel routes in routes/web.php",
    "version": "1.0.1",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "laravel",
        "php",
        "routes",
        "annotations"
    ],
    "activationEvents": [
        "onLanguage:php"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "laravelRoutes.toggle",
                "title": "Toggle Laravel Route Annotations"
            }
        ],
        "keybindings": [
            {
                "command": "laravelRoutes.toggle",
                "key": "ctrl+shift+t",
                "mac": "cmd+shift+t",
                "when": "editorTextFocus && resourceExtname == .php"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/vscode": "^1.74.0",
        "@types/node": "16.x",
        "typescript": "^4.9.4"
    }
}
